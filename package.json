{
  "name": "earthtrek-core",
  "version": "0.177.04",
  "description": "Global Nominee NASA Space Apps Challenge 2017 Buenos Aires - Global Finalist (Best Use of Data) NASA Space Apps Challenge 2017 - 1D, 2D, 3D, Go! Challenge",
  "homepage": "http://github.com/SATrek/EarthTrek",
  "author": {
    "name": "SATrek",
    "url": "http://github.com/SATrek"
  },
  "license": "Apache-2.0",
  "main": "earthtrek-dist.js",
  "keywords": [
    "earthtrek",
    "nasa",
    "gibs",
    "eos",
    "cesium",
    "satellites"
  ],
  "scripts": {
    "start": "node server.js",
    "compile": "babel --presets es2015,stage-0 --quiet -d dist/ src/",
    "dist": "mkdirp dist & rimraf dist/* && babel --presets es2015,stage-0 -d dist/ src/ && cpr src/css dist/css",
    "pubdist": "npm run dist && npm publish",
    "test": "mocha --compilers js:babel-core/register",
    "clean": "mkdirp public & rimraf public/*",
    "build": "npm run clean && cpr node_modules/cesium/Build/Cesium public && webpack --config example-src/config/webpack.config.js --progress",
    "devserver": "webpack-dev-server --config example-src/config/webpack.config.js --progress",
    "dev": "npm run build && npm run devserver",
    "release": "webpack --config config/webpack.prod.config.js --progress",
    "cache": "node ./src/tasks/cache-api.js",
    "heroku-postbuild": "npm run clean && cpr node_modules/cesium/Build/Cesium public && npm run release && npm run cache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SATrek/EarthTrek.git"
  },
  "bugs": {
    "url": "https://github.com/SATrek/EarthTrek/issues"
  },
  "contributors": [
    {
      "email": "alejandro.sanchez.trek@gmail.com",
      "name": "Alejandro Sanchez"
    },
    {
      "email": "impera87@gmail.com",
      "name": "Alejandro Hikari"
    },
    {
      "email": "leticia.tomasini@gmail.com",
      "name": "Leticia Tomasini"
    }
  ],
  "dependencies": {
    "cesium": "^1.32.1",
    "core-js": "^2.4.1",
    "express": "^4.15.2",
    "jquery": "^3.2.1",
    "jsonfile": "^3.0.0",
    "less-loader": "^3.0.0",
    "net": "^1.0.2",
    "query-string": "^4.3.4",
    "request": "^2.81.0",
    "request-promise": "^4.2.1",
    "satellite.js": "^1.3.0",
    "tle": "^1.2.2",
    "tls": "0.0.1",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "amd-loader": "0.0.8",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^6.2.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "core-js": "^2.4.1",
    "cpr": "^1.1.2",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "json-loader": "^0.5.4",
    "mkdirp": "^0.5.1",
    "mocha": "^3.3.0",
    "rimraf": "^2.5.2",
    "script-loader": "^0.7.0",
    "should": "^11.2.1",
    "style-loader": "^0.13.0",
    "uglify-js": "^2.8.27",
    "uglifyjs-webpack-plugin": "^0.4.3",
    "url-loader": "^0.5.8",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^4.1.0"
  }
}
